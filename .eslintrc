{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "airbnb"
  ],
  "plugins": [
    "babel",
    "import"
  ],
  "settings": {
    "import/ignore": [
      "node_modules",
      "\\.json$"
    ],
    "import/parser": "babel-eslint",
    "import/resolve": {
      "extensions": [
        ".js",
        ".jsx",
        ".json"
      ]
    }
  },
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-alert": 0,
    "no-console": 0,
    "no-undef": 1,
    "id-length": 0,
    "consistent-return": 0,
    "react/no-did-update-set-state": 1,
    "react/no-did-mount-set-state": 1,
    "react/jsx-first-prop-new-line": 0,
    "import/no-unresolved": 0,
    "max-len": [
      2,
      120,
      2,
      {
        "ignoreUrls": true,
        "ignoreComments": false
      }
    ],
    "new-cap": [
      2,
      {
        "capIsNewExceptions": [
          "express.Router",
          "Radium"
        ]
      }
    ],
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ]
  },
  "globals": {
    "logger": true,
    "debugLogger": true,
    "__SERVER__": true,
    "__CLIENT__": true,
    "__ENV__": true ,
    "__DEVTOOLS__": true,
    "__LOGGER__": true,
    "__DEVELOPMENT__": true,
    "__FIRST_PAGE_FLAG__": true
  }
}
